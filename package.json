{
  "name": "any-launcher-plus",
  "displayName": "Launcher Plus (Shortcuts)",
  "description": "Shortcut list to launch apps, scripts, or open docs from VS Code / Cursor / Windsurf. Tree view, QuickPick, variables, recent items.",
  "publisher": "PutraAdiJaya",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/PutraAdiJaya/any-launcher-plus.git"
  },
  "bugs": {
    "url": "https://github.com/PutraAdiJaya/any-launcher-plus/issues"
  },
  "homepage": "https://github.com/PutraAdiJaya/any-launcher-plus#readme",
  "keywords": [
    "launcher",
    "shortcuts",
    "automation",
    "scripts",
    "productivity",
    "runner"
  ],
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "launcher.openQuickPick",
        "title": "Launcher Plus: Open Shortcuts"
      },
      {
        "command": "launcher.run",
        "title": "Launcher Plus: Run Shortcut by ID",
        "category": "Launcher Plus",
        "enablement": "true"
      },
      {
        "command": "launcher.openSettings",
        "title": "Launcher Plus: Open Settings"
      },
      {
        "command": "launcher.exportShortcuts",
        "title": "Launcher Plus: Export Shortcuts (JSON)"
      },
      {
        "command": "launcher.importShortcuts",
        "title": "Launcher Plus: Import Shortcuts (JSON)"
      },
      {
        "command": "launcher.rescanAutoDiscover",
        "title": "Launcher Plus: Rescan Auto-Discovered Apps",
        "icon": "$(refresh)"
      },
      {
        "command": "launcher.generateTasks",
        "title": "Launcher Plus: Generate tasks.json from Shortcuts"
      },
      {
        "command": "launcher.setActiveProfile",
        "title": "Launcher Plus: Set Active Profile"
      },
      {
        "command": "launcher.exportShortcutsWorkspace",
        "title": "Launcher Plus: Export Shortcuts to Workspace (JSON)"
      },
      {
        "command": "launcher.importShortcutsWorkspace",
        "title": "Launcher Plus: Import Shortcuts into Workspace (JSON)"
      },
      {
        "command": "launcher.openEditor",
        "title": "Launcher Plus: Open Shortcut Editor"
      },
      {
        "command": "launcher.openSettingsFile",
        "title": "Launcher Plus: Open Settings File",
        "icon": "$(json)"
      },
      {
        "command": "launcher.validateShortcuts",
        "title": "Launcher Plus: Validate Shortcuts",
        "icon": "$(search)"
      },
      {
        "command": "launcher.clearCooldowns",
        "title": "Launcher Plus: Clear Cooldowns & Cache"
      },
      {
        "command": "launcher.cleanInvalidShortcuts",
        "title": "Launcher Plus: Clean Invalid Shortcuts",
        "icon": "$(trash)"
      },
      {
        "command": "launcher.searchShortcuts",
        "title": "Launcher Plus: Search Shortcuts",
        "icon": "$(search)"
      },
      {
        "command": "launcher.debug",
        "title": "Launcher Plus: Debug Info",
        "icon": "$(bug)"
      },
      {
        "command": "launcher.diagnoseLaunch",
        "title": "Launcher Plus: Diagnose App Paths",
        "icon": "$(search-view-icon)"
      },
      {
        "command": "launcher.testElectronSolutions",
        "title": "Launcher Plus: Test Electron Solutions",
        "icon": "$(beaker)"
      },
      {
        "command": "launcher.testLaunchStrategies",
        "title": "Launcher Plus: Test Launch Strategies",
        "icon": "$(rocket)"
      },
      {
        "command": "launcher.testVisibility",
        "title": "Launcher Plus: Test App Visibility",
        "icon": "$(eye)"
      }
    ],
    "configuration": {
      "title": "Launcher Plus",
      "properties": {
        "launcher.shortcuts": {
          "type": "array",
          "default": [],
          "markdownDescription": "List of shortcuts. Keys: id, label, program (empty uses OS default handler), args, cwd, env, runAsAdmin (Windows), when (VS Code when-clause-like condition), platform (win/mac/linux). Variables: ${file}, ${workspaceFolder}, ${relativeFile}, ${lineNumber}, ${selectedText}.\nNew fields: `icon` (codicon or absolute file path), `sequence` (array of inline steps or shortcut IDs) for multi-target execution.\nYou can set `profile` on a shortcut and control visibility with `launcher.activeProfile`. Also `sequenceMode`: `serial|parallel`."
        },
        "launcher.recentLimit": {
          "type": "number",
          "default": 10,
          "markdownDescription": "How many recent items to keep in memory."
        },
        "launcher.showInExplorer": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Show Shortcuts tree view in Explorer sidebar."
        },
        "launcher.enableAutoDiscover": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Automatically discover common apps (Chrome/Office/Notepad/etc.) and add virtual shortcuts (not saved into settings)."
        },
        "launcher.autoDiscoverPlatforms": {
          "type": "array",
          "default": [
            "win",
            "mac",
            "linux"
          ],
          "items": {
            "type": "string"
          },
          "markdownDescription": "Which platforms should attempt auto-discovery."
        },
        "launcher.activeProfile": {
          "type": "string",
          "default": "",
          "markdownDescription": "Active profile name. If a shortcut has `profile`, it will only show when this matches (or when profile is empty)."
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "launcher.shortcutsView",
          "name": "ðŸš€Launcher Plus",
          "when": "config.launcher.showInExplorer",
          "icon": "rocket"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "launcher.rescanAutoDiscover",
          "when": "view == launcher.shortcutsView",
          "group": "navigation"
        },
        {
          "command": "launcher.openSettingsFile",
          "when": "view == launcher.shortcutsView",
          "group": "navigation@2"
        },
        {
          "command": "launcher.searchShortcuts",
          "when": "view == launcher.shortcutsView",
          "group": "navigation@3"
        }
      ],
      "editor/context": [
        {
          "command": "launcher.openQuickPick",
          "group": "navigation@9"
        }
      ],
      "explorer/context": [
        {
          "command": "launcher.openQuickPick",
          "group": "navigation@9"
        }
      ]
    },
    "keybindings": [
      {
        "command": "launcher.openQuickPick",
        "key": "ctrl+alt+L",
        "mac": "cmd+alt+L",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepackage": "npm run compile",
    "package": "vsce package",
    "publish:ovsx": "ovsx publish",
    "publish:vsce": "vsce publish",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf out *.vsix",
    "prebuild": "npm run clean && npm run lint && npm run format:check",
    "build": "npm run compile && npm run package",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ovsx": "^0.9.2",
    "prettier": "^3.6.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.46.1",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "imagemagick": "^0.1.3"
  }
}
